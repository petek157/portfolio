<%= link_to('Back', projects_path()) %>
<h1><%= @project.title %></h1>
<p><%= @project.description %></p>
<% if @project.main_image.attached? %>
    <%= content_tag(:div, image_tag(@project.main_image, class: "list-item-image"), class: "list-item" ) %>
<% end %>
<% @project.image.each do |i| %>
    <%= content_tag(:div, image_tag(i, class: "list-item-image"), class: "list-item" )%>
<% end %>
<div class="teches">
    <% @project.teches.each do |tech| %>
        <div class="tech">
            <%= tech.name %>
        </div>
    <% end %>
</div>
<%= link_to('Edit', edit_project_path(id: @project.id, from: 's')) %>
<%= link_to('Delete', delete_project_path(@project.id)) %>
